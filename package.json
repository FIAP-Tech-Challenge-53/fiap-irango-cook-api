  "scripts": {
    "build": "rm -rf dist && nest build",
    "start": "node dist/infra/web/nestjs/main.js",
    "start:dev": "nest start --watch --preserveWatchOutput --entryFile=/infra/web/nestjs/main.js",
    "start:debug": "nest start --debug --watch --entryFile=/infra/web/nestjs/main.js",
    "start:prod": "node dist/infra/web/nestjs/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\" --format=stylish",
    "test:unit": "jest test/unit --config jest.config.unit.js",
    "test:unit:one": "jest $1 --config jest.config.unit.js --logHeapUsage",
    "test:coverage": "jest --runInBand --config jest.config.unit.js --verbose --coverage",
    "test": "npm run test:unit"
  },
